{
  "root": {
    "controller": "SystemController@redirectToLang",
    "routes": {
      "fr-FR": "/",
      "en-US": "/"
    }
  },
  "renderPluginTemplate": {
    "controller": "SystemController@renderPluginTemplate",
    "routes": {
      "fr-FR": "/renderPluginTemplate/*",
      "en-US": "/renderPluginTemplate/*"
    }
  },
  "home": {
    "controller": "StaticController@home",
    "routes": {
      "fr-FR": "/fr",
      "en-US": "/en"
    }
  },
  "admin": {
    "controller": "AdminController@display",
    "routes": {
      "fr-FR": "/fr/admin",
      "en-US": "/en/admin"
    }
  },
  "admin_": {
    "controller": "AdminController@display",
    "routes": {
      "fr-FR": "/fr/admin/*",
      "en-US": "/en/admin/*"
    }
  },
  "login": {
    "controller": "AuthController@login",
    "routes": {
      "fr-FR": "/fr/connexion",
      "en-US": "/en/login"
    }
  },
  "register": {
    "controller": "AuthController@register",
    "routes": {
      "fr-FR": "/fr/inscription",
      "en-US": "/en/register"
    }
  },
  "logout": {
    "controller": "AuthController@logout",
    "routes": {
      "fr-FR": "/fr/deconnexion",
      "en-US": "/en/logout"
    }
  },
  "password-forgot": {
    "controller": "AuthController@resetPassword",
    "routes": {
      "fr-FR": "/fr/mot-de-passe-oublie",
      "en-US": "/en/password-forgot"
    }
  },
  "api": {
    "controller": "ApiController@process",
    "api": true,
    "routes": {
      "fr-FR": "/api/*",
      "en-US": "/api/*"
    }
  }
}